   "source": [
    "### Write a program to find maximum between three numbers using conditional operator"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter input Values:3 0 6\n",
      "6 is max\n"
     ]
    }
   ],
   "source": [
    "a,b,c=input(\"Enter input Values:\").split(\" \")\n",
    "def maxnum():\n",
    "    if a>b and a>c:\n",
    "        print(a,\"is max\")\n",
    "    elif b>c:\n",
    "        print(b,\"is max\")\n",
    "    else:\n",
    "        print(c,\"is max\")\n",
    "maxnum()        "
   ]
  }
